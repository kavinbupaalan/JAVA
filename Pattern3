public class PrintPattern {
    public static void main(String[] args) {
        String[] patterns = {"A", "AL", "ALP", "ALPH"};
        
        for (int i = 0; i < patterns.length; i++) {
            for (int j = 0; j <= i; j++) {
                System.out.print(patterns[j]);
            }
            System.out.println();
        }
    }
}
